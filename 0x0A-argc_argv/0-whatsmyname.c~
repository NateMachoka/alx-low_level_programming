#include <stdio.h>
#include "main.h"

/**
 * main - a program that prints its name
 * @argv: the arguments passed in the program
 * @argc: number of arguments
 * Return: 0 success
 */

int main(int argc, char *argv[])
{
	(void) argc;
	char *program_name = argv[0];
	int i;

	for (i = 0; program_name[i] != '\0'; i++)
	{
		_putchar(program_name[i]);
	}

	_putchar('\n');

	return (0);
}
